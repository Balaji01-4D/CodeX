{
  "name": "kasca",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint:check": "turbo lint:check",
    "lint:fix": "turbo lint:fix",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test:e2e": "turbo test:e2e",
    "test:local": "turbo test:local",
    "test:remote": "turbo test:remote",
    "clean": "turbo clean && rimraf node_modules"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "prettier": "^3.4.2",
    "prettier-plugin-classnames": "^0.7.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rimraf": "^6.0.1",
    "turbo": "^2.3.3"
  },
  "packageManager": "pnpm@9.15.2",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.15.2"
  }
}
